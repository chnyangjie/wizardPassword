{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "settings": {
    "propWrapperFunctions": [
      "forbidExtraProps"
    ]
    // The names of any functions used to wrap the
    // propTypes object, e.g. `forbidExtraProps`.
    // If this isn't set, any propTypes wrapped in
    // a function will be skipped.
  },
  "plugins": [
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": 0,
    "strict": 0,
    "semi": [
      "error",
      "never"
    ],
    "quotes": [
      "error",
      "backtick"
    ]
  }
}

